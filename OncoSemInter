###  http://ontobci.org/ontology#TripleNegativeBreastCancer
:TripleNegativeBreastCancer rdf:type owl:Class ;
                            owl:equivalentClass [ owl:intersectionOf ( :BreastCancerDiagnosis
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty :hasReceptorStatus ;
                                                                         owl:someValuesFrom :EstrogenReceptorNegative
                                                                       ]
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty :hasReceptorStatus ;
                                                                         owl:someValuesFrom :HER2Negative
                                                                       ]
                                                                       [ rdf:type owl:Restriction ;
                                                                         owl:onProperty :hasReceptorStatus ;
                                                                         owl:someValuesFrom :ProgesteroneReceptorNegative
                                                                       ]
                                                                     ) ;
                                                  rdf:type owl:Class
                                                ] ;
                            rdfs:subClassOf :BreastCancerDiagnosis ;
                            :hasNCItMapping ncit:C77908 ;
                            :hasSNOMEDMapping <http://snomed.info/id/431396003> ;
                            rdfs:label "Cáncer de Mama Triple Negativo"@es ,
                                       "Triple Negative Breast Cancer"@en .


