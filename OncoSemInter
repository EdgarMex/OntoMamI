@prefix dcterms: <http://purl.org/dc/terms/> .
@base <http://ontobci.org/ontology#> .

<http://ontobci.org/ontology#> rdf:type owl:Ontology ;
                                dcterms:created "2025-09-29"^^xsd:date ;
                                dcterms:creator """Author: Francisco Edgar Castillo Barrera
Co-authors: Jose Emilio Labra Gayo""" ;
                                dcterms:description "An interoperability-focused ontology for breast cancer integrating multiple standards and data sources"@en ;
                                dcterms:license <https://creativecommons.org/licenses/by/4.0/> ;
                                dcterms:title "Breast Cancer Interoperability Ontology"@en ,
                                              "Ontología de Interoperabilidad para Cáncer de Mama"@es ;
                                owl:versionInfo "1.0.0" .

